effect set {
	fun set(n int)
}

fun set_setter(n int, setter handle set) {
	set(n) with setter
}

fun set_setter_try(n int) / set {
	set(n)
}

fun main() / debug {
	mut num = 69
	putint(num)

	let setter set = fun { n -> num = n }

	set(420) with setter
	putint(num)

	set_setter(24, setter)
	putint(num)

	set_setter_try(42) with setter
	putint(num)
}
