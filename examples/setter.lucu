effect set {
	fun set(n int)
}

fun set_setter(n int, setter set) {
	setter.set(n)
}

fun set_setter_try(n int) / set {
	set(n)
}

fun main() / debug {
	mut num = 69
	putint(num)

	try { num = 9000 }
	putint(num)

	let setter = handle set {
		fun set(n int) {
			num = n
		}
	}

	setter.set(420)
	putint(num)

	set_setter(24, setter)
	putint(num)

	with setter { set_setter_try(42) }
	putint(num)
}
