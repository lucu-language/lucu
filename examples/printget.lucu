import "core:io"

effect printget {
	fun print() / get
}

effect get {
	fun get() str
}

fun getter(s str) handle get {
	fun { s }
}
 
fun main() / io.stdio {
	try {

		print() with getter("one")
		print() with getter("two")
		print() with getter("tea")
		
	} with handle printget {

		fun print() / get {
			io.print(get())
		}
		
	}
}
