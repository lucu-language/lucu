import "core:io"

effect get {
    fun get() int
}

fun getter(n int) handle get {
	fun { n }
}

fun getter_indirect(n int) handle get {
	getter(n)
}

fun ident(h handle get) handle get {
	h
}

fun puttwice(n int) / io.stdio {
	let handler = getter_indirect(n)
	let handler = ident(handler)
	io.putint(get()) with handler
	io.putint(get()) with handler
}

fun main() / io.stdio {
	io.putint(420)
	puttwice(69)
}
