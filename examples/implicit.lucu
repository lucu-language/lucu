import "core:io"

effect get {
	fun get() int
}

handle get {
	fun get() int {
		69
	}
}

effect return {
	fun return(n int) !
}

handle return fails int {
	fun return(n int) ! {
		fail n
	}
}

fun main() / io.stdio {
	io.putint(try {
		io.putint(get())
		io.putint(get()) with handle get {
			fun get() int {
				420
			}
		}
		return(90)
		io.putint(get())
		100
	})
}
